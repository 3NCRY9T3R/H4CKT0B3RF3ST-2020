#include "iostream"
#include "algorithm"
#include "string"
#include "vector"
#include "queue"
#include "math.h"
#include "stdlib.h"
#include "ctype.h"
using namespace std;

int find(vector<int> a, int l, int h, int key)
{
    if (l > h)
        return -1;

    int mid = (l + h) / 2;
    if (a[mid] == key)
        return mid;

    if (key >= a[mid] && key <= a[h])
        return find(a, mid + 1, h, key);

    if (a[l] <= a[mid])
    {
        if (key >= a[l] && key <= a[mid])
            return find(a, l, mid - 1, key);
        return find(a, mid + 1, h, key);
    }

    return find(a, l, mid - 1, key);
}

int main()
{
    vector<int> a = {5, 6, 1, 2, 3};

    int key = 6;
    int i = find(a, 0, a.size() - 1, key);

    if (i != -1)
        cout << "Index: " << i << endl;
    else
        cout << "Number not found";
}
