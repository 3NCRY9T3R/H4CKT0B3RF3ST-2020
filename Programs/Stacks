#include <bits/stdc++.h>
#define int long long
using namespace std;
int32_t main() {
    int t;
    cin>>t;
    while(t--){
        int n,a;
        cin>>n;
        vector<int>v;
        for(int i=0;i<n;i++){
            cin>>a;
            if(v.size()==0){
                v.push_back(a);
                continue;
            }
            int idx=upper_bound(v.begin(),v.end(),a)-v.begin();
            if(idx<v.size()){
                v[idx]=a;
            }
            else{
                v.push_back(a);
            }
        }
        cout<<v.size()<<" ";
        for(auto x:v){
            cout<<x<<" ";
        }
        cout<<endl;
    }
    return 0;
}
